{
  "name": "@stackflow/plugin-history-sync",
  "version": "1.8.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/daangn/stackflow.git",
    "directory": "extensions/plugin-history-sync"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "yarn build:js && yarn build:dts",
    "build:dts": "tsc --emitDeclarationOnly",
    "build:js": "node ./esbuild.config.js",
    "clean": "rimraf dist",
    "dev": "yarn build:js --watch && yarn build:dts --watch && yarn test --watch",
    "test": "yarn jest",
    "typecheck": "tsc --noEmit"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "index.ts",
      "index.tsx"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  },
  "dependencies": {
    "flatted": "^3.3.1",
    "history": "^5.3.0",
    "react18-use": "^0.4.1",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "@graphql-tools/schema": "^10.0.5",
    "@stackflow/config": "^1.2.1",
    "@stackflow/core": "^1.3.0",
    "@stackflow/esbuild-config": "^1.0.3",
    "@stackflow/react": "^1.7.0",
    "@swc/core": "^1.6.6",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-relay": "^16.0.6",
    "@types/relay-runtime": "^17",
    "esbuild": "^0.23.0",
    "graphql": "^16.9.0",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "react-relay": "^17.0.0",
    "relay-compiler": "^17.0.0",
    "relay-runtime": "^17.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "@stackflow/config": "^1.0.1-canary.0",
    "@stackflow/core": "^1.1.0-canary.0",
    "@stackflow/react": "^1.3.2-canary.0",
    "@types/react": ">=16.8.0",
    "react": ">=16.8.0"
  },
  "relay": {
    "src": "./src",
    "schema": "./src/fixtures/graphql/schema.graphql",
    "language": "typescript",
    "artifactDirectory": "./src/fixtures/graphql/__generated__",
    "exclude": [
      "**/node_modules/**",
      "**/__generated__/**"
    ],
    "eagerEsModules": true
  },
  "publishConfig": {
    "access": "public"
  },
  "ultra": {
    "concurrent": [
      "dev",
      "build"
    ]
  }
}
