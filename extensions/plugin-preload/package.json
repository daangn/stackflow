{
  "name": "@stackflow/plugin-preload",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.js",
        "import": "./dist/index.mjs"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepack": "yarn build --clean",
    "build": "nanobundle build",
    "clean": "nanobundle clean",
    "dev": "tsc --emitDeclarationOnly --watch"
  },
  "dependencies": {
    "@stackflow/plugin-history-sync": "^1.0.0"
  },
  "devDependencies": {
    "@stackflow/core": "^1.0.0",
    "@stackflow/esbuild-config": "^0.20.0",
    "@stackflow/eslint-config": "^1.0.0",
    "@stackflow/react": "^1.0.0",
    "@types/react": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "nanobundle": "^1.4.0",
    "react": "^18.1.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@stackflow/core": "0",
    "@stackflow/react": "0",
    "@types/react": ">=16.8.0",
    "react": ">=16.8.0"
  },
  "ultra": {
    "concurrent": [
      "dev",
      "build"
    ]
  }
}
