{
  "name": "@stackflow/demo",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./index.css": "./dist/index.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn build:js && yarn build:dts && yarn build:docs",
    "build:docs": "vite build",
    "build:dts": "tsc --emitDeclarationOnly",
    "build:js": "node ./esbuild.config.js",
    "clean": "rimraf dist && rimraf build",
    "dev": "yarn build:js --watch && yarn build:dts --watch && yarn dev:app",
    "dev:app": "vite --host 0.0.0.0"
  },
  "dependencies": {
    "@seed-design/design-token": "^1.0.0-alpha.0",
    "@seed-design/stylesheet": "^1.0.0-alpha.0",
    "@stackflow/basic-ui": "^0.1.0",
    "@stackflow/core": "^0.1.0",
    "@stackflow/plugin-history-sync": "^0.1.0",
    "@stackflow/plugin-renderer-basic": "^0.1.0",
    "@stackflow/react": "^0.1.0",
    "@vanilla-extract/css": "^1.7.1",
    "lorem-ipsum": "^2.0.8",
    "normalize.css": "^8.0.1",
    "random-picture": "^4.0.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-lazy-load-image-component": "^1.5.4",
    "vite": "^2.9.9"
  },
  "devDependencies": {
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@vanilla-extract/css": "^1.7.1",
    "@vanilla-extract/vite-plugin": "^3.1.7",
    "@vitejs/plugin-react": "^1.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  },
  "ultra": {
    "concurrent": [
      "dev",
      "build"
    ]
  }
}
